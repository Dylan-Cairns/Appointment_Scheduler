<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ViewCustomersController">
   <children>
      <VBox layoutX="2.0" prefHeight="600.0" prefWidth="800.0">
         <children>
            <HBox alignment="CENTER_RIGHT" prefWidth="800.0">
               <children>
                  <HBox alignment="CENTER_LEFT" prefWidth="800.0">
                     <children>
                        <TextField fx:id="customerSearchTxtBox" alignment="TOP_RIGHT" prefHeight="30.0" prefWidth="250.0">
                           <HBox.margin>
                              <Insets left="30.0" right="10.0" />
                           </HBox.margin>
                        </TextField>
                        <Button fx:id="customerSearchButton" mnemonicParsing="false" onAction="#onActionSearchCustomer" prefHeight="30.0" prefWidth="80.0" text="Search">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" prefWidth="800.0">
                     <children>
                        <Button fx:id="viewCustomerButton11" mnemonicParsing="false" onAction="#onActionViewCustomer" prefWidth="80.0" text="View/Edit">
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="deleteCustomerButton11" mnemonicParsing="false" onAction="#onActionDeleteCustomer" prefWidth="80.0" text="Delete">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="addCustomerButton11" mnemonicParsing="false" onAction="#onActionAddCustomer" prefHeight="30.0" prefWidth="80.0" text="Add">
                           <HBox.margin>
                              <Insets right="30.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="15.0" top="30.0" />
               </padding>
            </HBox>
            <TableView prefHeight="489.0" prefWidth="740.0">
              <columns>
                <TableColumn fx:id="ViewCustTableviewNameCol" prefWidth="300.0" text="Name" />
                <TableColumn fx:id="ViewCustTableviewCityCol" prefWidth="200.0" text="City" />
                  <TableColumn fx:id="ViewCustTableviewStateCol" text="State" />
                  <TableColumn fx:id="ViewCustTableviewPhoneCol" prefWidth="160.0" text="Phone" />
              </columns>
               <VBox.margin>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
