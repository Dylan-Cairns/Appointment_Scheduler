<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.TextArea?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ViewReportsController">
   <children>
      <VBox alignment="CENTER">
         <children>
            <HBox alignment="CENTER">
               <padding>
                  <Insets bottom="15.0" top="30.0" />
               </padding>
               <children>
                  <HBox alignment="CENTER">
                     <children>
                        <Button fx:id="userApptSchedules" mnemonicParsing="false" onAction="#onActionuserApptSchedules" prefHeight="0.0" prefWidth="230.0" text="User appointment schedules">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="apptTypesPM" mnemonicParsing="false" onAction="#onActionApptTypesPM" prefHeight="0.0" prefWidth="230.0" text="Appointment types per month">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="avgApptLength" mnemonicParsing="false" onAction="#onActionAvgApptLength" prefHeight="0.0" prefWidth="230.0" text="Average Appointment length">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </HBox>
            <TextArea fx:id="ViewReportsTextBox" prefHeight="489.0" prefWidth="740.0">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TextArea>
            <HBox alignment="CENTER_RIGHT" prefWidth="800.0">
               <padding>
                  <Insets bottom="30.0" top="15.0" />
               </padding>
               <children>
                  <HBox alignment="CENTER_RIGHT" prefWidth="800.0">
                     <children>
                        <Button fx:id="backToMenuButton" mnemonicParsing="false" onAction="#onActionBackToMenu" prefHeight="30.0" prefWidth="80.0" text="Back">
                           <HBox.margin>
                              <Insets right="30.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
